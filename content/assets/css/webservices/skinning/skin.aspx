#logo a{
  background-image: url(/images/css/logo.png);
  text-indent: -9999px;
  height: 105px;
  overflow: hidden;
  display: block;
}
body{
  font: 12px Arial, Helvetica, sans-serif;
  background: #231f20;
}
#content a,
a{
  color: #8b0000;
}
#content a:hover,
a:hover{
  color: #8b0000;
}
h1{
  color: #666666;
  font-family: "Times New Roman", Times, serif;
}
h2{
  color: #666666;
  font-family: "Times New Roman", Times, serif;
}
h3{
  color: #666666;
  font-family: "Times New Roman", Times, serif;
}
h4{
  color: #666666;
  font-family: "Times New Roman", Times, serif;
}
h5{
  color: #666666;
  font-family: "Times New Roman", Times, serif;
}
h6{
  color: #666666;
  font-family: "Times New Roman", Times, serif;
}
.frame{
  border: 10px solid #FFFFFF;
  background: #FFFFFF;
}
#announcement{
  border: 5px solid #999999;
}
ul.detail_bar{
  background: #666666;
}
#download{
  background: #FFFFFF;
}
button,
.formButton,
.formButtonLong{
  color: #FFFFFF;
  background: #3F3F3F;
  border: 1px solid #000000;
}
#features .thumbs li{
  background: #222;
}
#utility_container{
  border-bottom: none;
}
#utility ul li a{
  color: #7d6a55;
}
.search{
  background: #FFFFFF;
  border-color: #D4D4D4;
}
.button{
  color: #000000;
  background: #D4D4D4;
  border-color: #BFBFBF;
}
#nav_container{
  border-top: none;
  border-bottom: 2px solid #7d6a55;
}
#nav ul{
  background: #8c0c04;
}
#nav ul li{
  font: 14px "Times New Roman", Times, serif;
  background: #8c0c04 url(/uploads/background_nav_li.gif) 0px 0px no-repeat;
  border-left: none;
}
#nav ul li a{
  color: #b5ab93;
}
#nav ul li:hover, 
#nav ul li.sfhover,
#nav ul li a:hover{
  color: #b5ab93;
  background: #8c0c04 url(/uploads/background_nav_li_open.gif) 0px 0px no-repeat;
}
#nav ul li ul{
  background: #8c0c04 url(/images/css/background-subnav.jpg);
}
#nav ul li ul li a{
  color: #b5ab93;
}
#nav ul li ul li a:hover{
  color: #b5ab93;
  background: #8c0c04;
}
#wide_container,
#full_container{
  border-top: none;
}
.border{
  border-top: 3px solid #E9E9E9;
}
#quote_container{
  border-bottom: 2px solid #7d6a55;
}
#quote{
  background: url(/images/css/background_quote.jpg) center;
}
#quote .verse{
  color: #FFFFFF;
  font: italic 21px "Times New Roman", Times, serif;
}
#quote .attribute{
  color: #CCCCCC;
  font: 15px "Times New Roman", Times, serif;
}
#content_wide #pageHeader{
  border-bottom: 2px solid #3F3F3F;
}
#content_wide #pageName h1,
#content_full #pageName h1{
  font: bold 34px "Times New Roman", Times, serif;
  border-bottom: 1px solid #7d6a55;
}
#content_wide #content_text,
#content_full #content_text{
  border-bottom: 1px solid #7d6a55;
}
#narrow_container{
  border-top: none;
}
#content_narrow{
  background: #b5ab93;
}
#content_narrow .image{
  background: #FFFFFF;
  border: 1px solid #7d6a55;
}
#group_nav,
#group_links{
  border-bottom: 1px solid #CCCCCC;
}
#group_nav .name h3{
  color: #666666;
  font: bold 21px "Times New Roman", Times, serif;
  border-top: 2px solid #7d6a55;
  border-bottom: 1px solid #7d6a55;
}
#group_nav ul li{
  font: bold 15px "Times New Roman", Times, serif;
}
#group_nav ul li a{
  color: #666666;
}
#group_nav ul li a:hover{
  color: #CCCCCC;
  background: #666666;
}
#group_nav .selected{
  color: #8c0c04;
}
#group_nav ul li a.cluster_parent{
  background: url(/uploads/bg_cluster_parent.gif) no-repeat 7px 9px;
}
#group_nav ul li a.cluster_parent:hover{
  background: #666666 url(/uploads/bg_cluster_parent.gif) no-repeat 7px 9px;
}
#group_nav ul li a.cluster_parent_open{
  background: #E9E9E9 url(/uploads/bg_cluster_parent_open.gif) no-repeat 7px 9px;
}
#group_nav ul li a.cluster_parent_open:hover{
  background: #666666 url(/uploads/bg_cluster_parent_open.gif) no-repeat 7px 9px;
}
#group_nav ul li ul{
  background: #E9E9E9;
}
#group_nav ul li ul li a{
  background: url(/uploads/bullet.gif) no-repeat 25px 11px;
}
#group_nav ul li ul li a:hover{
  background: #555555 url(/uploads/bullet.gif) no-repeat 25px 11px;
}
#group_nav ul li ul li a.selected{
}
#group_links ul{
  background: #CCCCCC;
}
#group_links ul li{
  font: bold 21px "Times New Roman", Times, serif;
}
#group_links ul li a{
  color: #666666;
}
#group_links ul li a:hover{
  color: #CCCCCC;
  background: #1F1F1f;
}
#footer_container{
  background: #b5ab93;
  border-top: 2px solid #3F3F3F;
}
#footer{
  background: #8c0c04;
}
#footer h1,
#footer table td h1{
  color: #D4D4D4;
  font: 12px "Times New Roman", Times, serif;
}
#footer,
#footer table td{
  color: #f3f4db;
}
#footer a,
#footer td a{
  color: #BFBFBF;
}
#so_link{
  background: #1F1F1F;
}
#so_link_text{
  color: #999999;
}
#so_link_text a{
  color: #CCCCCC;
}
#blogs_container{
  border-top: 3px solid #E9E9E9;
}
#RSS{
  color: #FFFFFF;
  background: #121212;
  border: 1px solid #000000;
}
#RSS .subscribe_rss a{
  color: #CCCCCC;
}
#author{
  background: #E9E9E9;
  border: 1px solid #DDDDDD;
}
#author .image{
  background: #FFFFFF;
  border: 1px solid #CCCCCC;
}
#article_menu .article_grouping ul li{
  border-bottom: 1px solid #DDDDDD;
}
#article_menu .article_grouping ul li a{
  background: url(/uploads/bullet.gif) no-repeat 5px 7px;
}
#article_menu .article_grouping ul li a:hover{
  color: #FFFFFF;
  background: #121212 url(/uploads/bullet.gif) no-repeat 5px 7px;
}
#articles .article,
#post .article{
  border-bottom: 3px #DDDDDD double;
}
.article .title{
  color: #121212;
  font: bold 15px Arial, Helvetica, sans-serif;
}
.article .footer{
  background: #E9E9E9;
  border: 1px solid #DDDDDD;
}
.comment{
  border-bottom: 3px #DDDDDD double;
}
.comment .date{
  color: #121212;
}
.comment .body{
  background: #FFFFFF;
  border: 1px solid #DDDDDD;
}
#post #news_link a,
#articles .view_all a{
  color: #FFFFFF;
  background: #555555;
  border: 1px solid #333333;
}
#post #news_link a:hover,
#articles .view_all a:hover{
  background: #121212;
  border: 1px solid #000000;
}
#content_narrow #blogs_container{
  border-top: 3px solid #E9E9E9;
}
#content_narroww #blogs_container h2{
  color: #666666;
  font: bold 24px "Times New Roman", Times, serif;
  border-bottom: 1px solid #CCCCCC;
}
#content_narrow #blogs .headline .title{
  color: #666666;
  font: bold 15px "Times New Roman", Times, serif;
}
#content_narrow  #blogs .headline .date{
  color: #949494;
  font: 11px "Times New Roman", Times, serif;
}
#content_narrow #blogs .link a{
  color: #666666;
  font: bold 16px "Times New Roman", Times, serif;
}
#content_narrow #blogs .link{
  border-bottom: 1px solid #CCCCCC;
}
#spotlight{
  margin: 20px 0;
  width: 920px;
}
#spotlight .block{
  display: block;
  float: left;
  height: 250px;
  margin: 0 13px;
  width: 275px;
}
#spotlight .block h2{
  display: block;
  margin: 5px 0;
}
#spotlight .block img{
  background-color: #ccc;
  border: 1px solid #fff;
  padding: 3px;
  position: relative;
}
#spotlight .block div{
  color: #000;
  display: block;
  font-size: 12px;
  margin: 5px 0;
}
#spotlight .block .link{
  border: none;
  color: #990000;
  display: block;
  text-align: right;
  text-decoration: none;
  text-transform: capitalize;
}
